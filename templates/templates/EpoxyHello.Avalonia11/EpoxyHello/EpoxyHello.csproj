<Project Sdk="Microsoft.NET.Sdk">
    
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFrameworks>net6.0</TargetFrameworks>
    <!-- <TargetFrameworks>net48;netcoreapp2.1;netcoreapp3.1;net5.0;net6.0;net7.0</TargetFrameworks> -->
    <LangVersion>latest</LangVersion>
    <Nullable>enable</Nullable>
    <IncludeAvaloniaGenerators>true</IncludeAvaloniaGenerators>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.3" PrivateAssets="All" />
    <PackageReference Include="Avalonia" Version="11.0.0-rc2.2" />
    <PackageReference Include="Avalonia.Desktop" Version="11.0.0-rc2.2" />
    <PackageReference Include="Avalonia.Diagnostics" Version="11.0.0-rc2.2" />
    <PackageReference Include="Avalonia.Themes.Fluent" Version="11.0.0-rc2.2" />
    <PackageReference Include="Epoxy.Avalonia11" Version="*" />
  </ItemGroup>
    
  <ItemGroup>
    <ProjectReference Include="..\EpoxyHello.Core\EpoxyHello.Core.csproj" />
  </ItemGroup>
    
</Project>
