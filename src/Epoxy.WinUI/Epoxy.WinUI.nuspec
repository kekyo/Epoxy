<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Epoxy.WinUI</id>
    <version>$PackageVersion$</version>
    <authors>Kouji Matsui (@kozy_kekyo, @kekyo2)</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">Apache-2.0</license>
    <licenseUrl>https://licenses.nuget.org/Apache-2.0</licenseUrl>
    <icon>Epoxy.100.png</icon>
    <projectUrl>https://github.com/kekyo/Epoxy.git</projectUrl>
    <description>An independent flexible XAML MVVM library for .NET</description>
    <releaseNotes>$PackageReleaseNotes$</releaseNotes>
    <copyright>Copyright (c) 2019-2021 Kouji Matsui</copyright>
    <tags>mvvm viewmodel easy simple combinable async awaitable command winui</tags>
    <repository type="git" url="https://github.com/kekyo/Epoxy.git" />
    <dependencies>
      <group targetFramework="net5.0-windows10.0.18362">
        <dependency id="Epoxy.Build" version="$PackageVersion$" exclude="All" />
        <dependency id="Epoxy.Core.WinUI" version="$PackageVersion$" exclude="Build,Analyzers" />
        <dependency id="Microsoft.WinUI" version="3.0.0-preview4.210210.4" exclude="Build,Analyzers" />
        <dependency id="System.Threading.Tasks.Extensions" version="4.5.4" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="bin\$Configuration$\**\Epoxy.dll" target="lib" />
    <file src="bin\$Configuration$\**\Epoxy.pdb" target="lib" />
    <file src="bin\$Configuration$\**\Epoxy.xml" target="lib" />
    <file src="..\..\Images\Epoxy.100.png" target="/" />
  </files>
</package>
